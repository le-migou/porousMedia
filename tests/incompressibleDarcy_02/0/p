FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    top
    {
        type            empty;
    }
    bottom
    {
        type            empty;
    }
    frontAndBack
    {
        type            empty;
    }
    right
    {
        type            fixedValue;
        value           uniform 0;
        // Or, if the velocity is prescribed at both the inlet and outlet:
        // (In this case a reference pressure must be set in system/fvSolution)
        // type            fixedFluxPressure;
    }
    left
    {
        type            fixedFluxPressure;
    }
}

// ************************************************************************* //
